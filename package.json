{
	"name": "@beneaththeink/appcore",
	"version": "3.0.1",
	"description": "A JavaScript application framework.",
	"private": true,
	"main": "lib/appcore.js",
	"repository": {
		"type": "git",
		"url": "https://beneaththeink.git.beanstalkapp.com/appcore.git"
	},
	"dependencies": {
		"object-path": "~0.9.2",
		"plain-merge": "~1.0.1",
		"asyncwait": "~1.0.2",
		"backbone-extend-standalone": "~0.1.2",
		"underscore": "~1.8.3"
	},
	"devDependencies": {
		"babel": "~5.8.23",
		"babelify": "~6.3.0",
		"grunt": "~0.4.5",
		"grunt-browserify": "~3.8.0",
		"grunt-cli": "~0.1.13",
		"grunt-contrib-clean": "~0.6.0",
		"grunt-contrib-concat": "~0.5.1",
		"grunt-contrib-uglify": "~0.9.1",
		"jshint": "~2.8.0",
		"tape": "~4.0.1"
	},
	"scripts": {
		"lint": "jshint src/ test/",
		"test": "npm run lint && npm run build-node && tape test/*.js",
		"test-browser": "browserify test/* -t babelify | browser-run -p 3000",
		"build-node": "rm -rf lib/ && babel src/ -d lib/",
		"build-browser": "grunt",
		"build": "npm run build-node && npm run build-browser",
		"prepublish": "npm run lint && npm run build"
	},
	"license": "MIT"
}
